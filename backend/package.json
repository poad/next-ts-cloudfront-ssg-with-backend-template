    "clean": "rimraf build",
    "prebuild": "pnpm lint"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.3.3",
    "@types/aws-lambda": "^8.10.125",
    "@types/node": "^20.8.10",
    "aws-cdk": "^2.103.1",
    "esbuild": "^0.19.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.103.1",
    "aws-lambda": "^1.0.7",
    "constructs": "^10.3.0",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "source-map-support": "^0.5.21"
  }
