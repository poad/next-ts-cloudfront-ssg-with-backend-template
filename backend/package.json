{
  "name": "@next-ts-cloudfront-ssg-with-backend-template/backend",
  "version": "0.0.1",
  "bin": {
    "backend": "bin/backend.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "format-check": "biome format bin lib lambda",
